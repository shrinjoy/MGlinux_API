{
  "name": "SmartWin",
  "productName": "SmartWin",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\"",
    "package-linux": "electron-packager . --platform=linux --arch=ia32 --icon=icons/icon256.png --out=release-builds",
    "make-win": "electron-forge make -- --arch=ia32,x64",
    "electron-build": "electron-builder build --win"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GTPang/MGDeluxeRelease.git"
  },
  "build": {
    "appId": "com.electron.smartwin",
    "asar": true,
    "mac": {
      "category": "multigame"
    },
    "icon": "icon.ico",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.4",
    "@babel/preset-env": "^7.24.4",
    "@babel/preset-react": "^7.24.1",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron-forge/plugin-webpack": "^7.4.0",
    "@electron-forge/publisher-github": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.11.0",
    "electron": "30.0.1",
    "electron-packager": "^17.1.2",
    "file-loader": "^6.2.0",
    "node-loader": "^2.0.0",
    "react-router-dom": "^6.22.3",
    "style-loader": "^3.3.4",
    "electron-builder": "^24.13.3",
    "electron-squirrel-startup": "^1.0.0",
    "electron-updater": "^6.1.8",
    "update-electron-app": "^3.0.0"
  },
  "keywords": [],
  "author": {
    "name": "webviosystem10",
    "email": "webviosystem10@gmail.com"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.8",
    "getmac": "^6.6.0",
    "internet-available": "^1.0.0",
    "react": "^18.2.0",
    "react-barcode": "^1.5.1",
    "react-clock": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-simple-image-slider": "^2.4.1",
    "react-spinners": "^0.13.8",
    "react-to-print": "^2.15.1",
    "react-toastify": "^10.0.5",
    "serial-number": "^1.3.0"
  }
}